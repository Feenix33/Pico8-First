    {  0,  0,  0,  0,  0,  0,  0,  0}, -- 9
    {  0,  0,  0,  0,  0,  0,  0,  0}, -- a
    {  0,  0,  0,  0,  0,  0,  0,  0}, -- b
    {  0,  0,  0,  0,  0,  0,  0,  0}  -- c
  }
  lawn = 
    {  0,  0,  0,118,118,  0,  0,  0}
end

function _init()
  preload() -- builds the arrays to clear
  build_scene()
  changed = true
end

function _draw()
end

function old_draw_scene()
  local v, h, arow
  local sx, sy
  cls(0)
  sy=0
  for v=1,vmax do
    sx=0
    for h=1,8 do
      if building[v][h] != 0 then
        spr(building[v][h], sx, sy)
      end
      if ornament[v][h] != 0 then
        palt(0, true)
        spr(ornament[v][h], sx, sy)
        palt(0, false)
      end
      sx+=8
    end
    for h=8,1,-1 do
      if building[v][h] != 0 then
        spr(building[v][h], sx, sy, 1, 1, true)
      end
      if ornament[v][h] != 0 then
        palt(0, true)
        spr(ornament[v][h], sx, sy, 1, 1, true)
        palt(0, false)
      end
      sx+=8
    end
    sy+=8
  end
  sy-=8
  sx=0
  for h=1,8 do
    if lawn[h] != 0 then
      palt(0, true)
      spr(lawn[h], sx, sy)
      palt(0, false)
    end
    sx+=8
  end
  for h=8,1,-1 do
    if lawn[h] != 0 then
      palt(0, true)
      spr(lawn[h], sx, sy, 1, 1, true)
      palt(0, false)
    end
    sx+=8
  end
  --draw_grass()
end

function _update()
  if changed then
    draw_scene()
    changed = false
  end
  if btn(4) then
    armed = true
  else
    if armed then
      build_scene()
      armed = false
    end
  end
end

