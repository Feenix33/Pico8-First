  building = {
    {  0,  0, 65, 65, 65, 65, 65, 65}, -- 1
    {  0,  0, 65, 65, 65, 65, 65, 65}, -- 2
    {  0,  0, 65, 65, 65, 65, 65, 65}, -- 3
    {  0,  0, 65, 65, 65, 65, 65, 65}, -- 4
    {  0,  0, 82, 67, 67, 67, 67, 67}, -- 5
