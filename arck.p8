pico-8 cartridge // http://www.pico-8.com
version 14
__lua__

--[[
-- 
]]--

ght = {}
gat = 0
gshp = {}
gen = {}
gback = 12 -- background color
gfly = true -- run game or not
gbull = {}
gbad = {}
gtime = (1+rnd(4))*15 -- random create bad guy

function log_start(cartname)
  local y,m,d=stat(90),stat(91),stat(92)
  local h,n=stat(93),stat(94)
  local months={'jan','feb','mar','apr','may','jun','jul','aug','sep','oct','nov','dec'}
  printh("run "..cartname.." @ "..d.."-"..months[m].."-"..y.."  "..h..":"..n)
end

function _init()
  srand(time())
  --log_start("cart")
  ght[0] = 96+flr(rnd(4))
  for j=1, 127 do
    --if flr(rnd(2)) == 0 then add(ght, ght[j-1]-1) else add(ght, ght[j-1]+1) end
    --add(ght, ght[j-1]+flr(rnd(3))-1)
    ght[#ght+1] = (ght[j-1]+flr(rnd(3))+127) %128
  end
  gshp.x = 8
  gshp.y = 64
  gshp.k = 10 -- kolor
  gshp.t = 39 -- type
  gshp.fire = false
  gfly = true
end

function check_collision()
  local  boom = false
  r={0, 7, 0, 7}
  c={0, 7, 7, 0}
  for j=1,4 do
    if pget(gshp.x+c[j], gshp.y+r[j]) != gback then
      boom = true
    end
  end
  ss = {}
  ss.x = (gshp.t % 16 * 8)
  ss.y = (flr(gshp.t/16) * 8)
  if boom then
    for r=0, 7 do
      for c=0, 7 do
        if sget(ss.x+r, ss.y+c) > 0 then
          if pget(gshp.x+r, gshp.y+c) != gback then
            --spr(gshp.t, gshp.x, gshp.y)
            --pset(gshp.x+r, gshp.y+c, 11)
            return true
          end
        end
      end
    end
  end
  return false
end

function bull_collision()
  for b in all(gbull) do
    for t in all(gbad) do
      if b.x >= t.x and b.x <= t.x+7 and b.y >= t.y and b.y <= t.y+7 then
        b.a = false
        t.a = false
        del(gbull, b)
        del(gbad, t)
      end
    end
  end
end

function bad_create()
  local b = {}
  b.x = 130
  b.y = rnd(90)
  b.a = true -- alive
  add(gbad, b)
end
function bad_draw(b)
  spr(36, b.x, b.y)
end
function bad_update(b)
  if flr(rnd(3)) == 0 then -- slow down updates
    if gshp.y < b.y then b.y-=1 end
    if gshp.y > b.y then b.y+=1 end
  end
  b.x -= 2
  if b.x < 0 then b.a = false end
end

function bull_create()
   local b = {}
   if gshp.fire == 1 then
     b.x = gshp.x+4
     b.y = gshp.y+4
     b.a = true
     add(gbull, b)
     gshp.fire = 2
   end
end
function bull_draw(b)
   pset(b.x, b.y, 8)
   pset(b.x-1, b.y, 8)
end
function bull_update(b)
   b.x += 2
   if b.x > 128 then 
     b.a = false 
     del(gbull, b)
   end
end
  
function fini_game()
  circfill(gshp.x+3, gshp.y+3, 4, 9)
  --spr(gshp.t, gshp.x, gshp.y)
  gfly = false
end

function _draw()
  if not gfly then return end
  cls(gback)
  local x=0
  for j=gat, 127 do
    line (x, ght[j], x, 128, 5)
    x+=1
  end
  for j=0, gat do
    line (x, ght[j], x, 128, 5)
    x+=1
  end
  foreach(gbull, bull_draw)
  foreach(gbad, bad_draw)

  -- check for collisions before drawing the ship
  if not gfly or check_collision() then
     fini_game()
  else
    spr(gshp.t, gshp.x, gshp.y)
  end
end

function handle_input()
  --if btnp(0) then sel.x -= 1 end -- left
  --if btnp(1) then sel.x += 1 end -- right
  if btn(2) then gshp.y -= 1 end -- up
  if btn(3) then gshp.y += 1 end -- down
  if btn(4) then gshp.fire += 1 else gshp.fire = 0 end 
end

function _update()
  if gfly then
    ght[gat] = ght[(gat+127)%128]+flr(rnd(3))-1
    gat = (gat+1)%128
    foreach(gbull, bull_update)
    foreach(gbad, bad_update)
    bull_collision()

    gtime -= 1
    if gtime <= 0 then
      gtime = (1+rnd(4))*15 -- random create bad guy
      bad_create()
    end

    handle_input()
    if gshp.fire == 1 then bull_create() end

  end
end

